<?xml version="1.0" encoding="utf-8"?>
<html>
  <head>
  </head>
  <body>
    <div type="splitlayout" name="divMain" layoutstyle="toptobottom" candragsplitter="true" dock="fill" size="400,400" splitterposition="0,100,400,101">
      <div location="0,0">
        <input type="usercontrol" cid="ribbonbutton" name="btnStart" text="开始" location="27,20" size="300,62" font="Arial,25" />
        <label name="lblStock1" text="股票1" location="344,20" size="70,30" font="Arial,25" forecolor="255,255,80" />
        <label name="Label2" text=":" location="470,14" size="10,35" font="Arial,30" />
        <label name="lblStock2" text="股票2" location="520,20" size="70,30" font="Arial,25" forecolor="255,0,255" />
        <input type="checkbox" name="cbNext" text="显示后续K线" location="691,63" size="104,17" />
        <input type="range" name="spinM" maximum="10000" location="495,62" size="48,20" textalign="Right" value="60" />
        <label name="Label3" text="参数M：" location="448,65" size="51,15" />
        <label name="Label4" text="参数N：" location="341,64" size="50,17" />
        <input type="range" name="spinN" location="390,62" size="46,20" textalign="Right" maximum="10000" value="5" />
        <input type="range" name="spinThreads" maximum="10000" location="621,62" size="48,20" textalign="Right" value="20" />
        <label name="Label5" text="线程数：" location="566,65" size="54,15" />
      </div>
      <div type="splitlayout" name="divChart" layoutstyle="lefttoright" candragsplitter="true" size="400,400" splitterposition="200,0,201,400">
        <div type="splitlayout" name="divCodes" layoutstyle="toptobottom" candragsplitter="true" dock="fill" size="400,400" splitterposition="0,50,400,50">
          <div>
            <label name="Label" text="过滤代码：" location="6,15" size="68,17" />
            <input type="text" name="txtCode" location="74,12" size="112,20" text="" />
          </div>
          <div type="splitlayout" name="divTable" layoutstyle="toptobottom" candragsplitter="true" size="400,400" splitmode="percentsize" splitterposition="0,200,400,200">
            <table name="tableCodes">
              <tr>
                <th text="股票代码" width="100" />
                <th text="最高得分" width="100" />
              </tr>
            </table>
            <table name="tableResults">
              <tr>
                <th text="得分" width="100" />
                <th text="相似代码" width="100" />
              </tr>
            </table>
          </div>
        </div>
	      <div type="tabcontrol" name="tabChart" selectedindex="0">
		      <div type="tabpage" text="相似K线">
			     <div type="splitlayout" name="divChart" dock="fill" layoutstyle="toptobottom" candragsplitter="true" size="400,400" splitmode="percentsize" splitterposition="0,200,400,200">
                <chart name="chartLeft">
                  <div VerticalPercent="0.7" />
                  <div VerticalPercent="0.3" />
                </chart>
                <chart name="chartRight">
                  <div VerticalPercent="0.7" />
                  <div VerticalPercent="0.3" />
                </chart>
              </div>
		      </div>
		      <div type="tabpage" text="选择区间">
			      <chart name="chartScope" dock="fill">
                  <div VerticalPercent="0.7" />
                  <div VerticalPercent="0.3" />
            </chart>
		      </div>
	      </div>
      </div>
    </div>
  </body>
</html>